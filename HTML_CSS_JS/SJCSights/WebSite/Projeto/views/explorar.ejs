<body>
  <div class="editAll escondido">
    <div class="editBallon">
      <button class="Xedit" onclick="closeEditar()"></button>

      <div class="escondido editInside">
        <div class="form__group">
          <input type="input" class="form__field" placeholder="Local" name="Local" id='Local' required />
          <label for="Local" class="form__label">Insira o local que você gostaria de editar</label>
        </div>
      </div>

      <button type="button" id='edit' onclick="AnalisarInputEditar()" class="edit escondido">Editar</button>
    </div>
  </div>


  <div class="all escondido">

    <div class="balao">
      <button class="X" onclick="closeAdicionar()"></button>
      <div class="tudo escondido">

        <form class="forms">
          <div class="tituloInput">
            <input type="text" id="title" name="title" placeholder="Título" class="input" required>
          </div>
          <div class="descricaoCurtaELonga">

            <textarea contenteditable="true" placeholder="Descrição Curta:" id="descricaoCurta" class="input"
              name="descricaoCurta" required></textarea>

            <textarea contenteditable="true" placeholder="Descrição Longa:" id="descricaoLonga" class="input"
              name="descricaoLonga" required></textarea>

          </div>

          <div class="Idade">
            <div class="label">Idade Recomendada</div>
            <div class="idades">
              <input type="number" id="idade1" name="idade1" placeholder="Mín" class="inputNumber" required>
              <input type="number" id="idade2" name="idade2" placeholder="Máx" class="inputNumber" required>
              <!-- <input type="number" id="avaliacao" name="avaliacao"  placeholder="Avaliação" class="inputNumber" required>  -->
            </div>
          </div>

          <div class="PalavrasChaves">

            <div class="esquerda">

              <div class="restauranteWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="restauranteSwitch" class="tgl tgl-skewed">
                  <label for="restauranteSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  ALIMENTAÇÃO
                </div>
              </div>

              <div class="familiaWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="familiaSwitch" class="tgl tgl-skewed">
                  <label for="familiaSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  FAMÍLIA
                </div>
              </div>

              <div class="esporteWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="esporteSwitch" class="tgl tgl-skewed">
                  <label for="esporteSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  ESPORTE
                </div>
              </div>

              <div class="turismoWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="turismoSwitch" class="tgl tgl-skewed">
                  <label for="turismoSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  TURISMO
                </div>
              </div>

              <div class="cinemaWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="cinemaSwitch" class="tgl tgl-skewed">
                  <label for="cinemaSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  CINEMA
                </div>
              </div>

            </div>

            <div class="direita">

              <div class="parqueWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="parqueSwitch" class="tgl tgl-skewed">
                  <label for="parqueSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  PARQUE
                </div>
              </div>

              <div class="comprasWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="comprasSwitch" class="tgl tgl-skewed">
                  <label for="comprasSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  COMPRAS
                </div>
              </div>

              <div class="hospedagemWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="hospedagemSwitch" class="tgl tgl-skewed">
                  <label for="hospedagemSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  HOSPEDAGEM
                </div>
              </div>

              <div class="estacionamentoWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="estacionamentoSwitch" class="tgl tgl-skewed">
                  <label for="estacionamentoSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  ESTACIONAMENTO
                </div>
              </div>

              <div class="festaWrap wrap">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="festaSwitch" class="tgl tgl-skewed">
                  <label for="festaSwitch" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitch">
                  FESTA
                </div>
              </div>

            </div>

          </div>

          <div class="EnderecoETelefone">
            <input type="text" id="Endereco" name="Endereco" placeholder="Endereço" class="input small" required>
            <input type="tel" id="Telefone" name="Telefone" placeholder="Telefone" class="input small" required>
          </div>

          <div class="Imagens">
            <input type="text" id="Imagens" name="Imagens" placeholder="Imagens" class="input" required multiple>
            <div class="help">?</div>
          </div>

          <div class="ButtonSpace">
            <button type="button" id='submit' class="submit">Enviar</button>
          </div>
        </form>
      </div>

    </div>

  </div>






  <div class="allFiltro escondido">

    <div class="balaoFiltro">
      <button class="XFiltro" onclick="closeFiltro()"></button>
      <div class="tudoFiltro escondido">

        <form class="formsFiltro">

          <div class="PalavrasChavesFiltro">

            <div class="esquerdaFilter">

              <div class="restauranteWrapFilter wrapFilter">
