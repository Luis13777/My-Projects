                  <label for="familiaSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  FAMÍLIA
                </div>
              </div>

              <div class="esporteWrapFilter wrapFilter">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="esporteSwitchFilter" class="tgl tgl-skewed" checked>
                  <label for="esporteSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  ESPORTE
                </div>
              </div>

              <div class="turismoWrapFilter wrapFilter">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="turismoSwitchFilter" class="tgl tgl-skewed" checked>
                  <label for="turismoSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  TURISMO
                </div>
              </div>

              <div class="cinemaWrapFilter wrapFilter">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="cinemaSwitchFilter" class="tgl tgl-skewed" checked>
                  <label for="cinemaSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  CINEMA
                </div>
              </div>

            </div>

            <div class="direitaFilter">

              <div class="parqueWrapFilter wrapFilter">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="parqueSwitchFilter" class="tgl tgl-skewed" checked>
                  <label for="parqueSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  PARQUE
                </div>
              </div>

              <div class="comprasWrapFilter wrapFilter">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="comprasSwitchFilter" class="tgl tgl-skewed" checked>
                  <label for="comprasSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  COMPRAS
                </div>
              </div>

              <div class="hospedagemWrapFilter wrapFilter">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="hospedagemSwitchFilter" class="tgl tgl-skewed" checked>
                  <label for="hospedagemSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  HOSPEDAGEM
                </div>
              </div>

              <div class="estacionamentoWrapFilter wrapFilter">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="estacionamentoSwitchFilter" class="tgl tgl-skewed" checked>
                  <label for="estacionamentoSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  ESTACIONAMENTO
                </div>
              </div>

              <div class="festaWrapFilter wrapFilter">
                <div class="checkbox-wrapper-8">
                  <input type="checkbox" id="festaSwitchFilter" class="tgl tgl-skewed" checked>
                  <label for="festaSwitchFilter" data-tg-on="SIM" data-tg-off="NÃO" class="tgl-btn"></label>
                </div>
                <div class="TextSwitchFilter">
                  FESTA
                </div>
              </div>

            </div>

          </div>






        </form>
      </div>

    </div>

  </div>























